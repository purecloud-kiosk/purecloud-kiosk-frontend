  "dependencies": {
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "fbemitter": "^2.0.0",
    "flux": "^2.1.1",
    "history": "^1.13.1",
    "http-proxy": "^1.13.0",
    "react": "^0.14.3",
    "react-date-picker": "^4.0.7",
    "react-dom": "^0.14.3",
    "react-notification-system": "^0.2.6",
    "react-notify": "^2.0.1",
    "react-popup": "^0.2.2",
    "react-popups": "^1.2.2",
    "react-router": "^1.0.2",
    "react-time-picker": "^1.1.0",
    "request": "^2.69.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "bower": "^1.6.5",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.2",
    "less": "^2.6.0",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "postinstall": "bower install",
    "build-js": "browserify jsx/index.jsx -t [babelify --presets [es2015 react] --extensions .jsx] --extension=.jsx -o dist/js/bundle.min.js",
    "watch-js": "watchify jsx/index.jsx -t [babelify --presets [es2015 react] --extensions .jsx] --extension=.jsx -o dist/js/bundle.min.js -v",
    "less": "lessc --clean-css less/index.less dist/css/dashboard.min.css",
    "watch-less": "watch 'npm run less' less",
    "watch": "npm run watch-js & npm run watch-less",
    "start-dev": "npm run watch & nodemon server.js",
    "start": "node server.js"
  },
